{
  "extends": ["tslint:latest", "tslint-config-prettier"],
  "exclude": ["node_modules"],
  "rules": {
    "no-bitwise": false,
    "object-literal-sort-keys": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ],
    "no-empty": [true, "allow-empty-functions"],
    "no-eval": false
  }
}
