{
  "name": "counters",
  "version": "1.0.0",
  "description": "An example implementation of counters using xynohm library.",
  "private": true,
  "engines": {
    "node": ">= 10.14.2"
  },
  "scripts": {
    "start": "npm-run-all clean:dist start:webpack",
    "start:webpack": "webpack-dev-server --hot --inline --config webpack/dev.config.js",
    "build:dev": "npm-run-all clean:dist test build:webpack:dev",
    "build:prod": "npm-run-all clean:dist test build:webpack:prod",
    "build:webpack:dev": "webpack --config webpack/dev.config.js",
    "build:webpack::prod": "webpack --config webpack/prod.config.js",
    "build:webpack:stats": "webpack --config webpack/stats.config.js",
    "type:check": "tsc",
    "lint:ts": "tslint -t verbose -p . -c tslint.json",
    "lint:css": "stylelint src/**/*.css --config ./.stylelintrc --formatter verbose",
    "clean:dist": "rm -rf dist/*",
    "clean:coverage": "rm -rf coverage",
    "test": "jest --config jest.config.js",
    "test:coverage": "jest --config jest.config.js --coverage",
    "test:watch": "jest --config jest.config.js --watchAll",
    "bundle:analyser": "npm-run-all clean:dist build:webpack:stats"
  },
  "author": "Alexander Zakharov",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-typescript": "^7.1.0",
    "@types/jest": "^23.3.11",
    "babel-loader": "^8.0.4",
    "base-href-webpack-plugin": "^2.0.0",
    "clean-css": "^4.2.1",
    "css-loader": "^2.1.0",
    "error-overlay-webpack-plugin": "^0.1.6",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "^1.15.3",
    "style-loader": "^0.23.1",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.2",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.2",
    "webpack": "^4.28.3",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.1.5"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@placardi/xynohm": "^0.3.6",
    "whatwg-fetch": "^3.0.0"
  }
}
